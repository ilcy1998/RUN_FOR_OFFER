    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "img = cv2.imread('F:\\\\cqy_smile_dataset\\\\zen_yang.jpg')\n",
    "\n",
    "h,w,c = img.shape\n",
    "\n",
    "M = np.float32([[1,0,200],[0,1,0]])\n",
    "img_ = cv2.warpAffine(img, M, dsize=(w,h))\n",
    "cv2.imshow('img_',img_)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
